{
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "GroupId": "elasticsearch-consumer-group",
    "GroupEventsTopic": "group-topic",
    "MessageEventsTopic": "message-topic",
    "AutoOffsetReset": "Earliest"
  },
  "Elasticsearch": {
    "Uri": "http://localhost:9200",
    "DefaultIndex": "app-index",
    "Username": "elastic",
    "Password": "elastic",
    "IsDebug": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
